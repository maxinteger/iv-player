# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf

[*.html]
indent_style = tab
indent_size = 4

[*.js]
indent_style = tab
indent_size = 4


[*.css]
indent_style = space
indent_size = 2
